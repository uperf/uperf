<?xml version="1.0"?>
<profile name="SpecWeb 2005 support">
	<group nthreads="750">
		<transaction iterations="1">
			<flowop type="connect" options="remotehost=$h3 protocol=tcp wndsz=256k"/>
		</transaction>
		<transaction duration="120s">
			<flowop type="read"  options="count=1 size=64"/>
			<flowop type="write" options="count=1 size=64k"/>
		</transaction>
		<transaction iterations="1">
			<flowop type="disconnect" />
		</transaction>
	</group>

