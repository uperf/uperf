<?xml version="1.0"?>
<profile name="simple.xml">
  <group nthreads="10">
        <transaction iterations="1">
            <flowop type="connect" options="remotehost=$h protocol=udp"/>
        </transaction>
        <transaction duration="3" rate="100">
            <flowop type="write" options="size=64"/>
        </transaction>
        <transaction iterations="1">
            <flowop type="disconnect" />
        </transaction>
  </group>
</profile>
