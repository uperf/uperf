<?xml version="1.0"?>
<profile name="sendfilev">
  <group nthreads="4">
        <transaction iterations="1">
            <flowop type="accept" options="remotehost=$h protocol=tcp
	    wndsz=50k  tcp_nodelay"/>
        </transaction>
        <transaction iterations="1">
            <flowop type="sendfile" options="dir=. nfiles=10"/>
        </transaction>
        <transaction iterations="1">
            <flowop type="disconnect" />
        </transaction>
  </group>

</profile>
